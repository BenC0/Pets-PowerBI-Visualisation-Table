{
    "dataRoles": [
        {
            "displayName": "Dimension",
            "name": "dimension",
            "kind": "Grouping"
        },
        {
            "displayName": "Metrics",
            "name": "metrics",
            "kind": "Measure"
        }
    ],
    "objects": {
        "ranking": {
            "properties": {
                "show_ranking": {
                    "type": {"bool": true}
                }
            }
        }
    },
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "dimension": {
                        "max": 2
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "for": { "in": "dimension" }
                },
                "values": {
                    "select": [
                        { "bind": { "to": "metrics" } }
                    ]
                }
            },
            "table": {
                "rows": {
                    "select": [
                        {
                            "for": {
                                "in": "column"
                            }
                        },
                        {
                            "for": {
                                "in": "value"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "privileges": [],
    "sorting": {
        "default": {   }
    }
}